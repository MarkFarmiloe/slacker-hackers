{
  "name": "starter-kit",
  "version": "0.0.1",
  "description": "Starter kit for full-stack JavaScript projects",
  "private": true,
  "scripts": {
    "prebuild": "npm run cleanup",
    "build": "npm run build:server && npm run build:client",
    "build:client": "webpack --config client/webpack/prod.config.js",
    "build:server": "babel server --out-dir dist",
    "cleanup": "rimraf ./dist/*",
    "dev": "concurrently -k -n \"client,server\" \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "webpack-dev-server --config client/webpack/dev.config.js",
    "dev:server": "cross-env PORT=3100 nodemon --inspect --watch server --exec babel-node server/server.js",
    "lint": "eslint .",
    "preserve": "npm run build",
    "serve": "npm start",
    "start": "node dist/server.js",
    "devstart": "react-scripts start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textbook/starter-kit.git"
  },
  "author": "Jonathan Sharpe <mail@jonrshar.pe>",
  "contributors": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textbook/starter-kit/issues"
  },
  "homepage": "https://github.com/textbook/starter-kit#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@codeyourfuture/eslint-config-standard": "^2.0.1",
    "axios": "^0.19.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.1.0",
    "eslint": "^7.5.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "html-webpack-tags-plugin": "^2.0.17",
    "nodemon": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "^3.0.2",
    "style-loader": "^1.2.1",
    "wait-on": "^5.1.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@nivo/bar": "^0.67.0",
    "@nivo/core": "^0.67.0",
    "@nivo/pie": "^0.67.0",
    "@slack/bolt": "^2.5.0",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "react-router-dom": "^5.2.0",
    "sql": "^0.78.0"
  },
  "engines": {
    "node": "^10 || ^12 || ^14"
  }
}
